{
    "name": "bsa/schedulia",
    "type": "project",
    "description": "Binary Studio Academy | 2020 | PHP group project. Schedulia is an analog of Ð¡alendly which purpose is to share your time and schedule events with others",
    "keywords": [
        "laravel",
        "bsa",
        "bsa2020",
        "php",
        "binarystudioacademy"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.148",
        "cviebrock/laravel-elasticsearch": "^4.2",
        "doctrine/dbal": "^2.10",
        "elasticsearch/elasticsearch": "^7.9",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "google/apiclient": "^2.7",
        "google/cloud-logging": "^1.20",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^4.4",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^4.1",
        "sentry/sentry-laravel": "1.8.0",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "Calendar",
            "Oauth2"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": "Google_Task_Composer::cleanup"
    }
}
